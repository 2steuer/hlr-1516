
set title "Lauftzeit in Abhaengigkeit der berechneten Interlines"
set xrange [0.5:2048]
set yrange [32:8000]

set xlabel "Zahl der Interlines"
set ylabel "Laufzeit in Sekunden"
unset key

set logscale x 2 
set logscale y 2

set xtics (1,2,4,8,16,32,64,128,256,512,1024) nomirror
set ytics auto nomirror

set grid

set style histogram clustered gap 1
set boxwidth 0.2 relative
set style fill  solid 0.5 noborder

set term postscript
set output "interlines.ps"
plot "interlines.dat" using 1:2 w boxes